<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<grid xsi:schemaLocation="http://isis.apache.org/applib/layout/component http://isis.apache.org/applib/layout/component/component.xsd" xmlns="http://isis.apache.org/applib/layout/grid/bootstrap3" xmlns:ns2="http://isis.apache.org/applib/layout/component" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <row>
        <col span="12" unreferencedActions="true">
            <ns2:domainObject/>
            <ns2:action id="recentChanges"/>
            <ns2:action id="downloadLayoutXml"/>
            <ns2:action id="downloadJdoMetadata"/>
            <ns2:action id="links"/>
        </col>
    </row>
    <row>
        <col span="4">
            <ns2:fieldSet name="Relationship" id="relationship" unreferencedProperties="true">
                <ns2:property id="from"/>
                <ns2:property id="to"/>
                <ns2:property id="relationshipType"/>
                <ns2:property id="description">
                    <ns2:action id="changeDescription" position="BELOW"/>
                </ns2:property>
                <ns2:property id="datanucleusVersionLong"/>
                <ns2:property id="datanucleusId"/>
                <ns2:property id="datanucleusVersionTimestamp"/>
            </ns2:fieldSet>
            <ns2:fieldSet name="Dates" id="dates">
                <ns2:property id="startDate"/>
                <ns2:property id="endDate">
                    <ns2:action id="changeDates" position="BELOW"/>
                </ns2:property>
            </ns2:fieldSet>
            <ns2:fieldSet name="Communication" id="communication"/>
            <ns2:fieldSet name="Application Level" id="applicationLevel"/>
        </col>
        <col span="8" unreferencedCollections="true"/>
    </row>
</grid>
